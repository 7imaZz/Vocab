<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.android.miwook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity
            android:name=".DictionaryActivity"
            android:label="Dictionary">
        </activity>
        <activity
            android:name=".VocabularyActivity"
            android:label="My Vocabulary"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PhrasesActivity"
            android:label="@string/category_phrases" />
        <activity
            android:name=".NounActivity"
            android:label="Nouns" />
        <activity
            android:name=".VerbActivity"
            android:label="Verbs" />
        <activity
            android:name=".AdjActivity"
            android:label="Adj&apos;s/Adv&apos;s" />
    </application>

</manifest>